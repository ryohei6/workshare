<p id="notice"><%= notice %></p>

<h1>Workshares</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workshares.each do |workshare| %>
      <tr>
        <td><%= workshare.title %></td>
        <td><%= workshare.body %></td>
        <td><%= link_to 'Show', workshare %></td>
        <td><%= link_to 'Edit', edit_workshare_path(workshare) %></td>
        <td><%= link_to 'Destroy', workshare, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workshare', new_workshare_path %>
